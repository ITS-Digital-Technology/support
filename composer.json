{
    "name": "northeastern-web/support",
    "description": "Package of support classes for Northeastern University websites",
    "keywords": [
        "northeastern-web",
        "support"
    ],
    "homepage": "https://github.com/ITS-Digital-Technology/support",
    "license": "MIT",
    "authors": [
        {
            "name": "Northeastern Web Solutions",
            "email": "websolutions@northeastern.edu",
            "homepage": "https://northeastern.edu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1|>=7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17"
    },
    "autoload": {
        "psr-4": {
            "Northeastern\\Support\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
